{
    "name": "brilliant-portal/nova-theme",
    "description": "A Laravel Nova theme.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "BrilliantPortal\\NovaTheme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BrilliantPortal\\NovaTheme\\ThemeServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "replace": {
        "brilliantportal/brilliantportal-nova-theme": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
